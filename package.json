{
  "name": "auto-captcha-submit-pro",
  "version": "7.3.0",
  "description": "This software for automatically submitting captcha code by eliminating mistakes for specific [*money.*] websites.",
  "main": "app.ts",
  "scripts": {
    "build": "webpack",
    "build:chrome": "webpack --env.target=chrome",
    "lint": "web-ext lint",
    "package": "npm run update-version && npm run build && web-ext build --overwrite-dest -a dist-firefox-package",
    "package:chrome": "npm run update-version && npm run build:chrome && web-ext build --overwrite-dest -s dist-chrome -a dist-chrome-package",
    "extension": "npm run build && web-ext run --source-dir=tests --firefox-profile=/home/abir/.mozilla/firefox/5gp4dglp.default-release --reload",
    "extension-dev": "npm run build && web-ext run --source-dir=tests --firefox-profile=/home/abir/.mozilla/firefox/kw3akca7.dev-edition-default --reload",
    "watch": "webpack -w",
    "watch:chrome": "webpack -w --env.target=chrome",
    "update-version": "node ./src/utils/sync-app-version",
    "zip-src": "node ./src/utils/zip-src"
  },
  "keywords": [
    "captcha-solver",
    "auto-captcha",
    "AutoCaptchaSubmit",
    "AutoCaptchaSubmitPro",
    "CaptchaSubmit",
    "MoneyEarning",
    "TrustedMoneyEaring"
  ],
  "author": "Mr Abir Ahamed",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "eslint": "^7.4.0",
    "file-loader": "^6.0.0",
    "javascript-obfuscator": "^0.28.4",
    "node-sass": "^4.14.1",
    "obfuscator-loader": "^1.1.2",
    "sass-loader": "^8.0.2",
    "semver-parser": "^3.0.5",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "unused-files-webpack-plugin": "^3.4.0",
    "web-ext-types": "^3.2.1",
    "web-ext-webpack-plugin": "github:hiikezoe/web-ext-webpack-plugin",
    "webextension-polyfill-ts": "^0.16.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-obfuscator": "^0.28.4",
    "webpack-preprocessor": "^0.1.12",
    "webpack-preprocessor-loader": "^1.1.2"
  },
  "webExt": {
    "sourceDir": "dist-firefox"
  },
  "dependencies": {
    "@types/jquery": "^3.3.38",
    "archiver": "^4.0.1",
    "html2canvas": "^1.0.0-rc.5",
    "jquery": "^3.5.1",
    "web-ext": "^4.2.0",
    "webrtc-adapter": "^7.6.1"
  }
}
